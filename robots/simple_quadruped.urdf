<?xml version="1.0"?><robot name="simple_quadruped"><link name="base_link"><visual><geometry><box size="1.0 1.0 0.05"/></geometry><material name="grey"><color rgba="0.5 0.5 0.5 1"/></material></visual><collision><geometry><box size="1.0 1.0 0.05"/></geometry></collision><inertial><mass value="5"/><inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/></inertial></link><!-- Front Left Leg -->
<joint name="hip_joint_fl" type="revolute">
    <parent link="base_link"/>
    <child link="thigh_fl"/>
    <axis xyz="1 0 0"/>
    <limit lower="-1.57" upper="1.57" effort="50" velocity="0.1"/>
    <origin rpy="0 0 0" xyz="0.5 0.5 0"/>
</joint>
<link name="thigh_fl">
    <visual>
        <origin rpy="0 0 0" xyz="0 0 -0.25"/>
        <geometry>
            <box size="0.05 0.05 0.5"/>
        </geometry>
        <material name="blue">
            <color rgba="0 0 1 1"/>
        </material>
    </visual>
    <collision>
        <origin rpy="0 0 0" xyz="0 0 -0.25"/>
        <geometry>
            <box size="0.05 0.05 0.5"/>
        </geometry>
        <contact>
            <lateral_friction value="0.9"/>
        </contact>
    </collision>
    <inertial>
        <mass value="1"/>
        <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <dynamics damping="0.1" friction="0.1"/>
</link>

