<?xml version="1.0" ?>
<!-- Optimized URDF for a quadruped with 4 joints (one per leg) -->
<robot name="servobot">
    <!-- Link mainbody -->
    <link name="mainbody">
        <inertial>
            <origin xyz="0.05515 0.00881981 0.081525" rpy="0 0 0"/>
            <mass value="1.5"/>
            <inertia ixx="0.003" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.0025"/>
        </inertial>
        <visual>
            <origin xyz="0.13515 0.021 0.156525" rpy="-1.5708 0 3.14159"/>
            <geometry>
                <mesh filename="base.stl"/>
            </geometry>
            <material name="base_material">
                <color rgba="0.615686 0.811765 0.929412 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.13515 0.021 0.156525" rpy="-1.5708 0 3.14159"/>
            <geometry>
                <mesh filename="base.stl"/>
            </geometry>
        </collision>
    </link>

    <!-- Link leg_br (Back Right) -->
    <link name="leg_br">
        <inertial>
            <origin xyz="0.0143552 0 -0.0172417" rpy="0 0 0"/>
            <mass value="0.08"/>
            <inertia ixx="3.2e-5" ixy="0" ixz="0" iyy="8.0e-5" iyz="0" izz="8.0e-5"/>
        </inertial>
        <visual>
            <origin xyz="0.0316745 0 0" rpy="1.5708 -0 -1.5708"/>
            <geometry>
                <mesh filename="botleg.stl"/>
            </geometry>
            <material name="leg_br_material">
                <color rgba="0.87451 0.447059 0.317647 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0316745 0 -0.02"/>
            <geometry>
                <sphere radius="0.01"/>
            </geometry>
            <contact>
                <lateral_friction value="0.9"/>
            </contact>
        </collision>
    </link>

    <!-- Link leg_bl (Back Left) -->
    <link name="leg_bl">
        <inertial>
            <origin xyz="0.0143552 0 -0.0172417" rpy="0 0 0"/>
            <mass value="0.08"/>
            <inertia ixx="3.2e-5" ixy="0" ixz="0" iyy="8.0e-5" iyz="0" izz="8.0e-5"/>
        </inertial>
        <visual>
            <origin xyz="0.0316745 0 0" rpy="1.5708 -0 -1.5708"/>
            <geometry>
                <mesh filename="botleg.stl"/>
            </geometry>
            <material name="leg_bl_material">
                <color rgba="0.87451 0.447059 0.317647 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0316745 0 -0.02"/>
            <geometry>
                <sphere radius="0.01"/>
            </geometry>
            <contact>
                <lateral_friction value="0.9"/>
            </contact>
        </collision>
    </link>

    <!-- Link leg_fr (Front Right) -->
    <link name="leg_fr">
        <inertial>
            <origin xyz="0.0143552 0 -0.0172417" rpy="0 0 0"/>
            <mass value="0.08"/>
            <inertia ixx="3.2e-5" ixy="0" ixz="0" iyy="8.0e-5" iyz="0" izz="8.0e-5"/>
        </inertial>
        <visual>
            <origin xyz="0.0316745 0 0" rpy="1.5708 -0 -1.5708"/>
            <geometry>
                <mesh filename="botleg.stl"/>
            </geometry>
            <material name="leg_fr_material">
                <color rgba="0.87451 0.447059 0.317647 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0316745 0 -0.02"/>
            <geometry>
                <sphere radius="0.01"/>
            </geometry>
            <contact>
                <lateral_friction value="0.9"/>
            </contact>
        </collision>
    </link>

    <!-- Link leg_fl (Front Left) -->
    <link name="leg_fl">
        <inertial>
            <origin xyz="0.0143552 0 -0.0172417" rpy="0 0 0"/>
            <mass value="0.08"/>
            <inertia ixx="3.2e-5" ixy="0" ixz="0" iyy="8.0e-5" iyz="0" izz="8.0e-5"/>
        </inertial>
        <visual>
            <origin xyz="0.0316745 0 0" rpy="1.5708 -0 -1.5708"/>
            <geometry>
                <mesh filename="botleg.stl"/>
            </geometry>
            <material name="leg_fl_material">
                <color rgba="0.87451 0.447059 0.317647 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0316745 0 -0.02"/>
            <geometry>
                <sphere radius="0.01"/>
            </geometry>
            <contact>
                <lateral_friction value="0.9"/>
            </contact>
        </collision>
    </link>

    <!-- Joints -->
    <joint name="BR_Hip" type="revolute">
        <origin xyz="0.1103 -0 0.16305" rpy="-3.14159 0 3.14159"/>
        <parent link="mainbody"/>
        <child link="leg_br"/>
        <axis xyz="0 0 1"/>
        <limit effort="1.8" velocity="6" lower="-1.5708" upper="1.5708"/>
        <dynamics damping="0.1" friction="0.1"/>
    </joint>

    <joint name="BL_Hip" type="revolute">
        <origin xyz="-0 -0 0.16305" rpy="3.14159 -0 -0"/>
        <parent link="mainbody"/>
        <child link="leg_bl"/>
        <axis xyz="0 0 1"/>
        <limit effort="1.8" velocity="6" lower="-1.5708" upper="1.5708"/>
        <dynamics damping="0.1" friction="0.1"/>
    </joint>

    <joint name="FR_Hip" type="revolute">
        <origin xyz="0.1103 -0 0" rpy="0 0 3.14159"/>
        <parent link="mainbody"/>
        <child link="leg_fr"/>
        <axis xyz="0 0 1"/>
        <limit effort="1.8" velocity="6" lower="-1.5708" upper="1.5708"/>
        <dynamics damping="0.1" friction="0.1"/>
    </joint>

    <joint name="FL_Hip" type="revolute">
        <origin xyz="-0.1742 -0.0237903 0.235556" rpy="1.5708 -0 0"/>
        <parent link="mainbody"/>
        <child link="leg_fl"/>
        <axis xyz="0 0 1"/>
        <limit effort="1.8" velocity="6" lower="-1.5708" upper="1.5708"/>
        <dynamics damping="0.1" friction="0.1"/>
    </joint>
</robot>